<p id="notice"><%= notice %></p>

<h1>Ethernet Logs</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Status</th>
      <th>Machine</th>
      <th>Tenant</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ethernet_logs.each do |ethernet_log| %>
      <tr>
        <td><%= ethernet_log.date %></td>
        <td><%= ethernet_log.status %></td>
        <td><%= ethernet_log.machine %></td>
        <td><%= ethernet_log.tenant %></td>
        <td><%= link_to 'Show', ethernet_log %></td>
        <td><%= link_to 'Edit', edit_ethernet_log_path(ethernet_log) %></td>
        <td><%= link_to 'Destroy', ethernet_log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ethernet Log', new_ethernet_log_path %>
