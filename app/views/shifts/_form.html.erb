<%= form_with(model: shift, local: true) do |form| %>
  <% if shift.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(shift.errors.count, "error") %> prohibited this shift from being saved:</h2>

      <ul>
      <% shift.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :working_time %>
    <%= form.text_field :working_time, id: :shift_working_time %>
  </div>

  <div class="field">
    <%= form.label :no_of_shift %>
    <%= form.number_field :no_of_shift, id: :shift_no_of_shift %>
  </div>

  <div class="field">
    <%= form.label :day_start_time %>
    <%= form.text_field :day_start_time, id: :shift_day_start_time %>
  </div>

  <div class="field">
    <%= form.label :isactive %>
    <%= form.check_box :isactive, id: :shift_isactive %>
  </div>

  <div class="field">
    <%= form.label :deleted_at %>
    <%= form.datetime_select :deleted_at, id: :shift_deleted_at %>
  </div>

  <div class="field">
    <%= form.label :working_time_dummy %>
    <%= form.time_select :working_time_dummy, id: :shift_working_time_dummy %>
  </div>

  <div class="field">
    <%= form.label :day_start_time_dummy %>
    <%= form.time_select :day_start_time_dummy, id: :shift_day_start_time_dummy %>
  </div>

  <div class="field">
    <%= form.label :tenant_id %>
    <%= form.text_field :tenant_id, id: :shift_tenant_id %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
