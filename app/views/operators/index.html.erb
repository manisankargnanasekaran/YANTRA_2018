<p id="notice"><%= notice %></p>

<h1>Operators</h1>

<table>
  <thead>
    <tr>
      <th>Operator name</th>
      <th>Operator spec</th>
      <th>Description</th>
      <th>Is active</th>
      <th>Deleted at</th>
      <th>Tenant</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @operators.each do |operator| %>
      <tr>
        <td><%= operator.operator_name %></td>
        <td><%= operator.operator_spec_id %></td>
        <td><%= operator.description %></td>
        <td><%= operator.is_active %></td>
        <td><%= operator.deleted_at %></td>
        <td><%= operator.tenant %></td>
        <td><%= link_to 'Show', operator %></td>
        <td><%= link_to 'Edit', edit_operator_path(operator) %></td>
        <td><%= link_to 'Destroy', operator, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Operator', new_operator_path %>
