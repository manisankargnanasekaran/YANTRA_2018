<%= form_with(model: connection_log, local: true) do |form| %>
  <% if connection_log.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(connection_log.errors.count, "error") %> prohibited this connection_log from being saved:</h2>

      <ul>
      <% connection_log.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :date %>
    <%= form.datetime_select :date, id: :connection_log_date %>
  </div>

  <div class="field">
    <%= form.label :status %>
    <%= form.text_field :status, id: :connection_log_status %>
  </div>

  <div class="field">
    <%= form.label :tenant_id %>
    <%= form.text_field :tenant_id, id: :connection_log_tenant_id %>
  </div>

  <div class="field">
    <%= form.label :machine_id %>
    <%= form.text_field :machine_id, id: :connection_log_machine_id %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
