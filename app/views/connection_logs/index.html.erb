<p id="notice"><%= notice %></p>

<h1>Connection Logs</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Status</th>
      <th>Tenant</th>
      <th>Machine</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @connection_logs.each do |connection_log| %>
      <tr>
        <td><%= connection_log.date %></td>
        <td><%= connection_log.status %></td>
        <td><%= connection_log.tenant %></td>
        <td><%= connection_log.machine %></td>
        <td><%= link_to 'Show', connection_log %></td>
        <td><%= link_to 'Edit', edit_connection_log_path(connection_log) %></td>
        <td><%= link_to 'Destroy', connection_log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Connection Log', new_connection_log_path %>
